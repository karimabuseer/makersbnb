<h1>Welcome to MakersBnB</h1>

  <% @listings.each do |listing| %>
    <ul>
      <li><%= listing.name %></li>
      <li><%= listing.description %></li>
      <li><%= listing.price %></li>
    </ul>
  <% end %>


<form action="/listings/add" method='get'>
  <input type="submit" value="Add listing">
</form>